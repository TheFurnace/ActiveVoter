<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  

  <div class="field">
    <%= f.label :zip %><br />
    <%= f.text_field :zip, autofocus: true %>
  </div>


  

  <div class="actions">
    <%= f.submit "Check voting stats" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
